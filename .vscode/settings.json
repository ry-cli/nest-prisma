{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "markdownlint.config": {
        "default": true,
        "MD010": {
            "code_blocks": false
        },
        "MD030": false
    },
    "workbench.editor.enablePreview": false,
    "compile-hero.disable-compile-files-on-did-save-code": true
}
